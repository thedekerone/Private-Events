<%= form_with model: @event do |f| %>
<%= f.label "name: ",:Event_Name %>
<%= f.text_field :name %><br><br>
<%= f.label "event_date: ",:Event_Date %>
<%= f.date_field :event_date %><br><br>
<%= f.label "description: ",:Description %>
<%= f.text_field :description %><br><br>
<%= f.submit "Create" %>
<%end%>